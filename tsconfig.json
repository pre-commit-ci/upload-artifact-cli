{
  "$schema": "https://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "rootDir": "./",
    "moduleResolution": "node",
    "baseUrl": "./",
    "outDir": "./dist",
    "noImplicitAny": true,
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "newLine": "lf"
  },
  "exclude": ["./dist", "./node_modules"]
}
